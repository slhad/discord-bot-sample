{
  "name": "discord-bot-sample",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node .",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "fix:build": "chmod +x node_modules/.bin/tsc",
    "full": "npm-run-all lint:fix build test"
  },
  "keywords": [],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slhad/discord-bot-sample.git"
  },
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "discord-api-types": "^0.33.0",
    "discord.js": "13.6.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.0",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": "error"
    }
  }
}